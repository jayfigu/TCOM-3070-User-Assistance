                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block writing-mode="lr">Order
                                                confirmation</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block writing-mode="lr">
                                                Shipping confirmation </fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block writing-mode="lr">
                                                Shipping update </fo:block></fo:list-item-body></fo:list-item>
                                        </fo:list-block>
                                    </fo:block></fo:block-container>
                                </fo:block>
